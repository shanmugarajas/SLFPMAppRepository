<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
    elementFormDefault="qualified" 
    xmlns:resourceimport="http://slf.com/pmapp/service/xsd/resourceimport-schema"
    targetNamespace="http://slf.com/pmapp/service/xsd/resourceimport-schema">

    <xs:element name="GetResourceImportRequest">
        <xs:complexType>
            <xs:all>
                <xs:element name="name" type="xs:string" maxOccurs="1"
                    minOccurs="1" />
                <xs:element name="empid" type="xs:string" maxOccurs="1"
                    minOccurs="1" />
                <xs:element name="empstatus" type="xs:string" maxOccurs="1"
                    minOccurs="1" />
                <xs:element name="dojslf" type="xs:date" maxOccurs="1"
                    minOccurs="1" />
                <xs:element name="email" type="xs:string" maxOccurs="1"
                    minOccurs="1" />
                <xs:element name="mobile" type="xs:int" maxOccurs="1"
                    minOccurs="1" />
                <xs:element name="phone" type="xs:int" maxOccurs="1"
                    minOccurs="1" />
            </xs:all>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="GetResourceImportResponse">
        <xs:complexType>
            <xs:all>
                <xs:element name="value" type="xs:int" maxOccurs="1"
                    minOccurs="1" />
                <xs:element name="response" type="xs:string" maxOccurs="1"
                    minOccurs="1" />
            </xs:all>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="GetResourceImportFault">
        <xs:complexType>
            <xs:all>
                <xs:element name="faultMessage" type="xs:string"
                    maxOccurs="1" minOccurs="1" />
            </xs:all>
        </xs:complexType>
    </xs:element>
</xs:schema>
